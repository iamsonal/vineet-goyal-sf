{
    "name": "@salesforce/lds-adapters-analytics-data-service",
    "version": "1.37.10",
    "description": "The Einstein Analytics family of APIs",
    "license": "MIT",
    "main": "dist/umd/es2018/analytics-data-service.js",
    "module": "dist/es/es2018/analytics-data-service.js",
    "types": "dist/types/src/generated/artifacts/main.d.ts",
    "files": [
        "dist",
        "sfdc",
        "src/raml/*"
    ],
    "contributors": [
        "vlad.eroshin@salesforce.com",
        "pmcarthur@salesforce.com",
        "seth.white@salesforce.com",
        "adam.hancock@salesforce.com",
        "ryarovoy@salesforce.com",
        "ssathyamoorthy@salesforce.com"
    ],
    "sfdc": {
        "path": "forcelds/ldsAdaptersAnalyticsDataService/"
    },
    "scripts": {
        "clean": "rm -rf dist sfdc src/generated karma/dist",
        "build": "yarn build:raml && yarn build:services && yarn build:karma",
        "build:karma": "rollup --config rollup.config.karma.js",
        "build:raml": "luvio generate src/raml/luvio.raml src/generated  -p '../lds-compiler-plugins'",
        "build:services": "rollup --config rollup.config.js",
        "start": "karma start",
        "test": "karma start --single-run",
        "test:debug": "karma start --single-run=false",
        "test:compat": "karma start --single-run --compat",
        "release:corejar": "yarn build && ../core-build/scripts/core.js --adapter=lds-adapters-analytics-data-service",
        "release:core": "../../scripts/release/core.js --adapter=lds-adapters-analytics-data-service"
    },
    "dependencies": {
        "@salesforce/lds-bindings": "1.37.10"
    },
    "devDependencies": {
        "@luvio/cli": "0.72.0",
        "@luvio/compiler": "0.72.0",
        "@luvio/engine": "0.72.0",
        "@luvio/lwc-luvio": "0.72.0",
        "@salesforce/lds-karma": "1.37.10"
    },
    "volta": {
        "extends": "../../package.json"
    }
}
