        type: string
      label:
        description: The label of the asset.
        type: string
        required: false # TODO Hand-rolled W-8253396
      lastAccessedDate:
        description: Last time the asset was accessed.
        type: string | nil
        required: false # TODO Hand-rolled W-8253396
      lastModifiedBy:
        description: The user that last updated the asset.
        type: WaveUserRepresentation | nil
        required: false # TODO Hand-rolled W-8253396
      lastModifiedDate:
        description: Last time the asset was modified.
        type: string | nil
        required: false # TODO Hand-rolled W-8253396
      name:
        description: The name of the asset.
        type: string
        required: false # TODO Hand-rolled W-8253396
      namespace:
        description: The namespace of the Asset.
        type: string
        required: false # TODO Hand-rolled W-8253396
      permissions:
        description: Represents permissions for the present user.
        type: PermissionsRepresentation | nil
        required: false # TODO Hand-rolled W-8253396
      type:
        description: The asset type.
        type: string
        enum:
          - Component
          - Dashboard
          - DashboardSnapshot
          - DataConnector
          - DataPoolEntity
          - DataPoolEntityVersion
          - Dataflow
          - DataflowJob
          - DataflowJobNode
          - Dataset
          - DatasetShard
          - DatasetVersion
          - ExternalData
          - Folder
          - Lens
          - Recipe
          - ReplicatedDataset
          - Story
          - Workflow
      url:
        description: URL to get the definition of the asset.
        type: string
