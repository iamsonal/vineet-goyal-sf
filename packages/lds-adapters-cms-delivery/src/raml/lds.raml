#%RAML 1.0 Overlay
extends: ./api.raml

uses:
  lds: lds://annotations.raml

(lds.keyPrefix): 'CMS::'

types:
  ManagedContentChannelCollectionRepresentation:
    (lds.opaque): true
  ManagedContentVersionCollectionRepresentation:
    (lds.opaque): true

/connect/cms/delivery/channels:
  get:
    (lds.adapter):
      name: getDeliveryChannels
/connect/cms/delivery/channels/{channelId}/contents:
  /query:
    get:
      (lds.adapter):
        name: listContent
