/connect:
  /omnistudio:
    /evaluation-services:
      get:
        description: Used for fetching calculation procedure from source object.
        responses:
          '200':
            description: Success
            body:
              application/json:
                type: CalculationProcedureListOutputRepresentation
        queryParameters:
          searchKey:
            type: string
            required: false
    /evaluation-services/version-definitions:
      post:
        description: API to save Calculation Procedure version Definition
        responses:
          '200':
            description: Success
            body:
              application/json:
                type: CalculationProcedureVersionDefinitionOutputRepresentation
        body:
          application/json:
            # TODO Hand-rolled W-8334626
            type: CalculationProcedureVersionDefinitionInputWrapperRepresentation
            # TODO: Hand-rolled due to issue #22 in Generated RAML Gaps (https://salesforce.quip.com/NxVhAkxL6RTW#dGQACAxyxIv)
            #required: false
            (oas-body-name): calculationProcedureDefinition
    /evaluation-services/version-definitions/{id}:
      get:
        description: API to get Calculation Procedure version Definition
        responses:
          '200':
            description: Success
            body:
              application/json:
                type: CalculationProcedureVersionDefinitionOutputRepresentation
      patch:
        description: API to activate Calculation Procedure Version
        responses:
          '200':
            description: Success
            body:
              application/json:
                type: CalculationProcedureActivationOutputRepresentation
        queryParameters:
          action:
            type: string
            required: false
      uriParameters:
        id:
          type: string
          required: true
    /evaluation-services/{id}:
      get:
        description: Get information about a single calc proc object.
        responses:
          '200':
            description: Success
            body:
              application/json:
                type: CalculationProcedureDetailOutputRepresentation
      uriParameters:
