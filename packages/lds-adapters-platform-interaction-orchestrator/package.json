{
    "name": "@salesforce/lds-adapters-platform-interaction-orchestrator",
    "version": "1.38.1",
    "description": "Interaction Orchestrator APIs",
    "license": "MIT",
    "main": "dist/umd/es2018/platform-interaction-orchestrator.js",
    "module": "dist/es/es2018/platform-interaction-orchestrator.js",
    "types": "dist/types/src/generated/artifacts/main.d.ts",
    "files": [
        "dist",
        "sfdc",
        "src/raml/*"
    ],
    "contributors": [
        "tstokes@salesforce.com",
        "aaron.liebling@salesforce.com"
    ],
    "scripts": {
        "build": "yarn build:raml && yarn build:services && yarn build:karma",
        "build:karma": "rollup --config rollup.config.karma.js",
        "build:raml": "luvio generate src/raml/luvio.raml src/generated -p '../lds-compiler-plugins'",
        "build:services": "rollup --config rollup.config.js",
        "clean": "rm -rf dist sfdc src/generated karma/dist",
        "start": "karma start",
        "test": "karma start --single-run",
        "test:compat": "karma start --single-run --compat",
        "release:core": "../../scripts/release/core.js --adapter=lds-adapters-platform-interaction-orchestrator"
    },
    "dependencies": {
        "@salesforce/lds-bindings": "1.38.1"
    },
    "devDependencies": {
        "@luvio/cli": "0.73.8",
        "@luvio/compiler": "0.73.8",
        "@luvio/engine": "0.73.8",
        "@luvio/lwc-luvio": "0.73.8",
        "@salesforce/lds-karma": "1.38.1"
    }
}
