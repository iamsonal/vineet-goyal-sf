{
    "name": "@salesforce/lds-network-aura",
    "version": "1.26.0",
    "description": "LDS Network Adapter for Aura Runtime",
    "private": true,
    "license": "MIT",
    "main": "dist/ldsNetwork.js",
    "module": "dist/ldsNetwork.js",
    "types": "dist/main.d.ts",
    "sfdc": {
        "path": "forcelds/ldsNetwork/",
        "publishedFileName": "ldsNetwork.js"
    },
    "scripts": {
        "prepare": "yarn build",
        "build": "rollup --config rollup.config.js",
        "clean": "rm -rf dist",
        "test:unit": "jest",
        "test:debug": "node --inspect-brk ../../node_modules/.bin/jest --runInBand",
        "release:core": "yarn build && ../core-build/scripts/core.js --name=lds-network-aura"
    },
    "devDependencies": {
        "@salesforce/lds-adapters-uiapi": "1.26.0",
        "@salesforce/lds-aura-storage": "1.26.0",
        "@salesforce/lds-environment-settings": "1.26.0",
        "@salesforce/lds-instrumentation": "1.26.0"
    }
}
